{
"explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "v":"open:vsplit",
    "L":"copyRelativeFilepath",
    "q":"quickfix:replace",
    "Q":"quickfix:add",
    "g[": ["wait", "markPrev:diagnosticError"],
    "g]": ["wait", "markNext:diagnosticError"]
  },
 "coc.preferences.formatOnSaveFiletypes": ["go","javascript", "typescript","javascriptreact","typescriptreact","python","css","scss","html","lua"],
  "eslint.enable": true,
  "explorer.icon.enableNerdfont": true,
  "eslint.autoFixOnSave":true,
  "eslint.alwaysShowStatus":true,
  "prettier.semi": true,
  "python.linting.pylintEnabled":false,
  "python.formatting.provider":"black",
  "python.linting.lintOnSave":true,
  "pyright.inlayHints.functionReturnTypes":false,
  "pyright.inlayHints.variableTypes":false
}


